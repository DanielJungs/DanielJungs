            <nav class="col-12 col-md-4 bg-dark text-white p-3">
                <h2>Menu Navigasi</h2>
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">Features</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="#">Pricing</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
                    </li>
                </ul>
            </nav>

            <!-- Konten Utama -->
            <main class="col-md-8">
                <h2>Main Content</h2>
                <p>Ini adalah area konten utama.</p>
            </main>
        </div>

        <!-- Sampingan (Sidebar) -->
        <div class="row">
            <aside class="col-md-4 offset-md-8 bg-light p-3">
                <h3>Sidebar</h3>
                <p>Ini adalah area sampingan.</p>
